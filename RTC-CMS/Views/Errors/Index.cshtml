@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
@using Microsoft.AspNetCore.Http
@using RTC_CMS.Common
@using RTC_CMS.Models
@{
    var user = Context.Session.GetObject<Users>("User");
    ViewData["Title"] = "Error";
    ViewData["NavHeader"] = "Error";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/css/errors/errors.css" rel="stylesheet" />
<div class="px-1 px-md-2 py-3">
    <div class="container-fluid px-1 px-md-2">
        <h1 class="my-3">Lỗi</h1>
        <div class="card mb-4">
            <div class="card-header">
                <button class="btn btn-primary" id="error_add">
                    <i class="fa-solid fa-plus"></i>
                </button>
            </div>
            <div class="card-body">
                <div id="errors_table" class="table-bordered border-primary"></div>
            </div>
        </div>
    </div>
</div>
<script src="~/js/errors/errors.js"></script>